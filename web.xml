        <servlet-name>SeeOrderServlet</servlet-name>
        <url-pattern>/SeeOrderServlet</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>GuestServlet</servlet-name>
        <url-pattern>/GuestServlet</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>UserTrackingServlet</servlet-name>
        <url-pattern>/UserTrackingServlet</url-pattern>
    </servlet-mapping>
